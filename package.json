{
  "name": "velas-desktop-wallet",
  "version": "0.11.03",
  "description": "Velas Wallet",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "ls": "electron-builder --help",
    "pack": "electron-builder --dir",
    "dist": "electron-builder --publish always",
    "dist-nix": "electron-builder --linux",
    "dist-deb": "electron-builder --linux deb tar.xz",
    "dist-win": "electron-builder --win portable",
    "rebuild": "electron-rebuild -f -w web3-wallet",
    "postinstall": "electron-builder install-app-deps"
  },
  "repository": "https://web3.space",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "license": "CC0-1.0",
  "devDependencies": {
    "app-builder-bin": "^3.5.2",
    "electron": "^7.1.11",
    "electron-builder": "^22.3.2",
    "electron-builder-notarize": "^1.1.2",
    "electron-notarize": "^0.2.1",
    "electron-rebuild": "1.8.4"
  },
  "author": "Andrey Stehno <a.stegno@gmail.com>",
  "build": {
    "framework": "electron",
    "afterSign": "electron-builder-notarize",
    "appId": "web3.space",
    "mac": {
      "hardenedRuntime": true,
      "gatekeeperAssess": false,
      "entitlements": "./node_modules/electron-builder-notarize/entitlements.mac.inherit.plist"
    }
  },
  "dependencies": {
    "@primer/octicons-react": "^9.4.0",
    "atob": "^2.1.2",
    "big-integer": "^1.6.45",
    "bignumber.js": "^6.0.0",
    "bip39": "^2.5.0",
    "bitcoinjs-lib": "^3.3.2",
    "bitgo-utxo-lib": "^1.8.0",
    "bs58": "^4.0.1",
    "cross-fetch": "^3.0.4",
    "crypto-js": "^3.1.9-1",
    "eosjs": "^16.0.9",
    "ethereumjs-tx": "^1.3.7",
    "ethereumjs-util": "^5.2.0",
    "ethereumjs-wallet": "^0.6.3",
    "ethers": "^4.0.44",
    "libsodium-wrappers": "^0.7.6",
    "libsodium-wrappers-sumo": "^0.7.6",
    "localStorage": "^1.0.3",
    "mobx": "^4.1.0",
    "mobx-react": "^5.0.0",
    "moment": "^2.22.2",
    "prelude-ls": "^1.1.2",
    "protect": "^0.5.0",
    "qrcode.react": "^0.8.0",
    "qs": "^6.5.2",
    "react": "^16.2.0",
    "react-copy-to-clipboard": "^5.0.1",
    "react-dom": "^16.2.0",
    "react-google-recaptcha": "^2.0.1",
    "require-ls": "0.0.8",
    "solidity-sha3": "^0.4.1",
    "ssl-root-cas": "^1.3.1",
    "superagent": "^3.8.3",
    "superagent-proxy": "^2.0.0",
    "web3": "^0.20.1"
  }
}
