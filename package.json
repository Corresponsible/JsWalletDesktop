{
  "name": "velas-desktop-wallet",
  "homepage": "velas.com",
  "version": "0.12.43",
  "description": "Velas Wallet",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "ls": "electron-builder --help",
    "pack": "electron-builder --dir",
    "dist": "electron-builder --publish always",
    "dist-nix": "electron-builder --linux",
    "dist-deb": "electron-builder --linux deb tar.xz",
    "dist-win": "electron-builder --win portable",
    "dist-mac": "electron-builder --mac",
    "build": "electron-builder --windows --linux",
    "release": "electron-builder --windows --linux --publish always",
    "rebuild": "electron-rebuild -f -w web3-wallet",
    "postinstall": "electron-builder install-app-deps",
    "install": "pwd && cp scripts/pre-commit .git/hooks/ && chmod u+x .git/hooks/pre-commit && chmod u+x scripts/bump-version.sh"
  },
  "repository": {
    "type" : "git",
    "url" : "https://github.com/velas/JsWalletDesktop"
  },
  "repository_prev_value": "https://web3.space",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "license": "CC0-1.0",
  "devDependencies": {
    "app-builder-bin": "^3.5.2",
    "electron": "^7.1.11",
    "electron-builder": "latest",
    "electron-builder-notarize": "^1.1.2",
    "electron-notarize": "^0.2.1",
    "electron-rebuild": "1.8.4"
  },
  "author": "Andrey Stehno <a.stegno@gmail.com>",
  "build": {
    "framework": "electron",
    "afterSign": "electron-builder-notarize",
    "appId": "web3.space",
    "mac": {
      "hardenedRuntime": true,
      "gatekeeperAssess": false,
      "entitlements": "./node_modules/electron-builder-notarize/entitlements.mac.inherit.plist"
    },
    "dmg": {
			"icon": false
		},
		"linux": {
			"target": ["AppImage"],
			"category": "Utility"
		}
  },
  "dependencies": {
    "@primer/octicons-react": "^9.6.0",
    "atob": "^2.1.2",
    "big-integer": "^1.6.48",
    "bignumber.js": "^6.0.0",
    "bip39": "^2.5.0",
    "bitcoinjs-lib": "^3.3.2",
    "bs58": "^4.0.1",
    "btoa": "^1.2.1",
    "chart.js": "^2.9.3",
    "cross-fetch": "^3.0.4",
    "crypto-js": "^3.3.0",
    "ethereum-common": "^0.2.1",
    "ethereumjs-common": "^1.5.0",
    "ethereumjs-util": "^5.2.0",
    "hdkey": "^1.1.2",
    "libsodium-wrappers-sumo": "^0.7.6",
    "localStorage": "^1.0.4",
    "md5": "^2.2.1",
    "mobx": "^4.15.4",
    "mobx-react": "^5.4.4",
    "moment": "^2.22.2",
    "prelude-ls": "^1.2.1",
    "protect": "^0.5.0",
    "qrcode.react": "^0.8.0",
    "qs": "^6.5.2",
    "react": "^16.13.1",
    "react-chartjs-2": "^2.9.0",
    "react-copy-to-clipboard": "^5.0.2",
    "react-dom": "^16.2.0",
    "require-ls": "0.0.8",
    "scryptsy": "^1.2.1",
    "secp256k1": "^4.0.0",
    "ssl-root-cas": "^1.3.1",
    "superagent": "^3.8.3",
    "superagent-proxy": "^2.0.0",
    "uuid": "^7.0.3",
    "web3": "^0.20.1"
  }
}
